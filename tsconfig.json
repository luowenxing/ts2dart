{
  "compilerOptions": {
    "target": "ES2016", 
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "allowUnreachableCode": false,
    "lib": ["es2015", "es2020"]
  },
  "exclude": [
    "node_modules",
    "build",
    "test/e2e",
    "test/tsc_e2e"
  ]
}
